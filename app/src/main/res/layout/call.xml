<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:weightSum="3"
    android:gravity="center"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <Button
        android:background="#71D50000"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/a333"
        android:gravity="center"
        android:textSize="15dp"
        android:textColor="#000000"
        android:textStyle="normal"
        android:layout_gravity="center"
        android:text="National Call center 333"
        android:autoLink="phone"
        />
    <Button
        android:layout_height="0dp"
        android:layout_weight="1"
        android:textSize="15dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textStyle="normal"
        android:background="#5000C853"
        android:layout_width="match_parent"
        android:id="@+id/a10605"
        android:autoLink="phone"
        android:text="Health 10605"
        />
    <Button
        android:layout_height="0dp"
        android:layout_weight="1"
        android:textSize="15dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textStyle="normal"
        android:background="#5E6200EA"
        android:layout_width="match_parent"
        android:id="@+id/a16263"
        android:autoLink="phone"
        android:text="IEDCR 16263"
        />

</LinearLayout>